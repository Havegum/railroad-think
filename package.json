  "license": "MIT",
  "type": "module",
  "description": "null",
  "postcss": {
    "plugins": {
      "postcss-preset-env": {
        "stage": 0
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid",
    "semi": true,
    "svelteIndentScriptAndStyle": false
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "deploy": "yarn build && ./deploy.sh && bord4 integrate",
