    "@sveltejs/adapter-static": "^1.0.0-next.39",
    "@sveltejs/kit": "^1.0.0-next.431",
    "@tsconfig/svelte": "^3.0.0",
    "@types/d3-array": "^3.0.4",
    "@vanilla-extract/css": "^1.7.2",
